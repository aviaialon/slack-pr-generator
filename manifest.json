{
	"name": "Slackify Github Pull Requests",
	"version": "0.1",
	"manifest_version": 2,
	"description": "",
	"background": {
		"scripts": ["background.js"]
	},
	"permissions": [
		"activeTab",
		"http://*/*",
		"https://*/*"
	],
	"content_scripts": [
		{
			"matches": ["https://github.com/*"],
			"js": ["jquery.js"],
			"run_at": "document_end"
		}
	],
	"icons": {
		"16": "images/icon-16.png",
		"48": "images/icon-48.png",
		"128": "images/icon-128.png"
	},
	"browser_action": {
		"default_icon": {
			"48": "images/icon-48.png"
		},
		"default_title": "Slackify Github Pull Requests"
	}
}